<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.UserDao">
    <!--配置查询所有-->
    <select id="findAll" resultType="com.domain.User">
        <!--id为Dao接口方法的名称-->
        /*select内部写sql语句*/
        select * from user;
    </select>
</mapper>

